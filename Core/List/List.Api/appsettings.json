{
  "IdentityServer": "http://host.docker.internal:35105",
  "IdentityServerHealthCheck": "http://recall-identityapi/hc",
  "TextListHealthCheck": "http://recall-envoygateway/text/hc",
  "MaskedTextListHealthCheck": "http://recall-envoygateway/maskedtext/hc",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "SeqServerUrl": "http://recall-seq",
    "LogstashgUrl": null,
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "AllowedHosts": "*"
}
